# SonarQube Configuration
SONARQUBE_URL=http://localhost:9000
SONARQUBE_TOKEN=your-sonarqube-token-here
SONARQUBE_PROJECT_KEY=your-project-key-here
SONARQUBE_ORGANIZATION=your-organization-key-here

# Export Configuration
EXPORT_OUTPUT_PATH=./reports
EXPORT_FILENAME=sonarqube-issues-report.html
EXPORT_EXCLUDE_STATUSES=CLOSED
EXPORT_INCLUDE_RESOLVED=false
EXPORT_MAX_ISSUES=10000
EXPORT_TEMPLATE=default

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/export.log

# Development Configuration (if running as server)
PORT=3000
NODE_ENV=development
